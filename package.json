{
  "name": "kanban",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "style-injector": "style2js --out-dir dist dist/style.css",
    "build": "tsc && vite build && npm run style-injector",
    "preview": "vite preview",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.21",
    "@vitejs/plugin-react": "^1.1.4",
    "acorn-jsx": "^5.3.2",
    "babel-loader": "^8.2.3",
    "consola": "^3.0.1",
    "json": "^11.0.0",
    "style2js": "^1.0.1",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vite-plugin-dts": "^0.9.9",
    "vite-plugin-style-import": "^2.0.0"
  },
  "license": "UNLICENSED",
  "peerDependencies": {
    "react": "^16.14.0 || 17.x",
    "react-dom": "^16.14.0 || 17.x"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/kanban.umd.js",
  "module": "./dist/kanban.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/kanban.es.js",
      "require": "./dist/kanban.umd.js"
    },
    "./dist/inject-style": {
      "import": "./dist/inject-style.esm.mjs",
      "require": "./dist/inject-style.umd.js"
    }
  }
}